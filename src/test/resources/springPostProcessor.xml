<?xml version="1.0" encoding="UTF-8"?>
<beans>

    <bean id="userDao" class="com.kayson.test.bean.UserDao"/>

    <bean id="userService" class="com.kayson.test.bean.UserService" init-method="initDataMethod" destroy-method="destroyDataMethod">
        <property name="uId" value="10001"/>
        <property name="company" value="腾讯"/>
        <property name="location" value="深圳"/>
        <property name="userDao" ref="userDao"/>
    </bean>

    <bean class="com.kayson.test.common.MyBeanPostProcessor"/>
    <bean class="com.kayson.test.common.MyBeanFactoryPostProcessor"/>

    <bean id="productService" class="com.kayson.test.bean.ProductService">
        <property name="uId" value="10001"/>
        <property name="userDao" ref="userDao"/>
    </bean>
</beans>